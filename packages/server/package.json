{
  "name": "@trpc/server",
  "version": "9.10.0",
  "description": "tRPC Server",
  "author": "KATT",
  "license": "MIT",
  "homepage": "https://trpc.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trpc/trpc.git",
    "directory": "packages/server"
  },
  "eslintConfig": {
    "rules": {
      "explicit-module-boundary-types": "off"
    }
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "adapters/express.ts",
      "adapters/next.ts",
      "adapters/node-http/index.ts",
      "adapters/standalone.ts",
      "adapters/ws.ts",
      "rpc/index.ts",
      "ws/index.ts"
    ]
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "default": "./dist/trpc-server.cjs.js",
      "types": "./dist/trpc-server.cjs.d.ts"
    },
    "./adapters/express": {
      "default": "./adapters/express/dist/trpc-server-adapters-express.cjs.js",
      "types": "./adapters/express/dist/trpc-server-adapters-express.cjs.d.ts"
    },
    "./adapters/next": {
      "default": "./adapters/next/dist/trpc-server-adapters-next.cjs.js",
      "types": "./adapters/next/dist/trpc-server-adapters-next.cjs.d.ts"
    },
    "./adapters/node-http": {
      "default": "./adapters/node-http/dist/trpc-server-adapters-node-http.cjs.js",
      "types": "./adapters/node-http/dist/trpc-server-adapters-node-http.cjs.d.ts"
    },
    "./adapters/standalone": {
      "default": "./adapters/standalone/dist/trpc-server-adapters-standalone.cjs.js",
      "types": "./adapters/standalone/dist/trpc-server-adapters-standalone.cjs.d.ts"
    },
    "./adapters/ws": {
      "default": "./adapters/ws/dist/trpc-server-adapters-ws.cjs.js",
      "types": "./adapters/ws/dist/trpc-server-adapters-ws.cjs.d.ts"
    },
    "./rpc": {
      "default": "./rpc/dist/trpc-server-rpc.cjs.js",
      "types": "./rpc/dist/trpc-server-rpc.cjs.d.ts"
    },
    "./ws": {
      "default": "./ws/dist/trpc-server-ws.cjs.js",
      "types": "./ws/dist/trpc-server-ws.cjs.d.ts"
    }
  },
  "scripts": {
    "test": "jest"
  },
  "main": "dist/trpc-server.cjs.js",
  "module": "dist/trpc-server.esm.js",
  "typings": "dist/trpc-server.cjs.d.ts",
  "files": [
    "README.md",
    "dist",
    "adapters",
    "ws",
    "rpc",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/express": "^4.17.12",
    "@types/hash-sum": "^1.0.0",
    "@types/ws": "^8.2.0",
    "devalue": "^2.0.1",
    "express": "^4.17.1",
    "fastify": "^3.17.0",
    "hash-sum": "^2.0.0",
    "jest": "^27.1.0",
    "kont": "^0.5.1",
    "myzod": "^1.3.1",
    "next": "^11.1.0",
    "superstruct": "^0.15.2",
    "typescript": "4.4.4",
    "ws": "^8.0.0",
    "yup": "^0.32.8",
    "zod": "^3.0.0"
  },
  "gitHead": "82bf01f8759b04f2ade69c9047e28f74f889d547",
  "dependencies": {
    "tslib": "^2.1.0"
  }
}
